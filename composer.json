{
  "name": "thejawker/laravel-route-module-macro",
  "description": "Allows for easy unique code generation, is database aware for Laravel Applications.",
  "keywords": [
    "thejawker",
    "random",
    "unique",
    "database-aware-unique-code-generator",
    "token-generator",
    "code-generator"
  ],
  "homepage": "https://github.com/thejawker/laravel-route-module-macro",
  "license": "MIT",
  "authors": [
    {
      "name": "Bram Veerman",
      "email": "bram.veerman@gmail.com",
      "homepage": "https://github.com/thejawker",
      "role": "Developer"
    }
  ],
  "require": {
    "php": ">=7.0.0",
    "illuminate/support": "~5.5.0",
    "illuminate/validation": "~5.5.0",
    "illuminate/pagination": "~5.5.0",
    "illuminate/view": "~5.5.0"
  },
  "require-dev": {
    "mockery/mockery": "^0.9.5",
    "orchestra/database": "~3.5.0",
    "orchestra/testbench": "~3.5.0",
    "phpunit/phpunit": "^6.2"
  },
  "autoload": {
    "psr-4": {
      "TheJawker\\SuperRandom\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "TheJawker\\SuperRandom\\Test\\": "tests"
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit"
  },
  "config": {
    "sort-packages": true
  },
  "extra": {
    "laravel": {
      "providers": [
        "TheJawker\\SuperRandom\\SuperRandomServiceProvider"
      ],
      "aliases": {
        "SuperRandom": "TheJawker\\SuperRandom\\SuperRandom"
      }
    }
  }
}
